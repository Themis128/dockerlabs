# Python backend server URL
# This is the URL where the Python server (server.py) is running
PYTHON_SERVER_URL=http://localhost:3000

# API base URL
# This is the base URL for API calls from the Nuxt frontend
# In development, this is proxied through Vite
API_BASE_URL=http://localhost:3000/api

# Nuxt server port (optional, defaults to 3000)
# PORT=3001

# Node environment
# NODE_ENV=development

# Enable verbose logging (optional)
# VERBOSE=false
